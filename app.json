{
  "expo": {
    "name": "vivoduoc",
    "slug": "vivoduoc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#012C56",
      "statusBar": {
        "style": "light"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "backgroundColor": "#012C56",
      "barStyle": "light-content"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode" : "pan",
      "package": "com.evairx.vivoduoc"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["assets/fonts/OpenSans.tff"]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ae9a11b7-d6b2-4edd-8306-8b31f0b96bed"
      }
    },
    "owner": "evairx"
  }
}
